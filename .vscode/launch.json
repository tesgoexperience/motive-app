  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug in Exponent",
      "request": "launch",
      "type": "reactnative",
      "cwd": "${workspaceFolder}",
      "platform": "exponent",
      "expoHostType": "lan",
      "enableDebug": true,
      "skipFiles": [
        "node_modules/**",
      ],
      "env": {"REST_API":"http://192.168.1.199:8080"},
      "openExpoQR": false,
    }
  ]
}
